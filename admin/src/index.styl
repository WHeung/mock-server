body,
html
  margin 0
  padding 0
  min-height 100%
  background #f7f7f7
  font-family "Helvetica Neue",Helvetica,Arial,sans-serif

div,p,ul,ol,li,span,a,button,input,textarea,form
  margin:0
  padding:0

button,input,textarea,form
  border 0
i
  font-style normal
ul,ol
  list-style:none

input, button, textarea, select
  font-family inherit
  font-size inherit
  font-weight inherit
  border 0
  background none
  -webkit-appearance none
  outline none

a
  text-decoration none
